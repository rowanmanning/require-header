{
  "name": "require-header",
  "version": "6.1.0",
  "description": "Express/connect middleware to handle errors where an expected header is missing",
  "keywords": [
    "400",
    "error",
    "express",
    "header",
    "http",
    "middleware"
  ],
  "author": "Rowan Manning (https://rowanmanning.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/rowanmanning/require-header.git"
  },
  "homepage": "https://github.com/rowanmanning/require-header",
  "bugs": "https://github.com/rowanmanning/require-header/issues",
  "license": "MIT",
  "engines": {
    "node": "22.x || 24.x"
  },
  "scripts": {
    "verify": "npm run verify:biome && npm run verify:types",
    "verify:biome": "biome check",
    "verify:types": "tsc --noEmit --project ./jsconfig.json",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "node --test --experimental-test-coverage --test-coverage-branches=100 --test-coverage-functions=100 --test-coverage-lines=100 test/unit/**/*.test.js",
    "test:integration": "node --test test/integration/**/*.test.js",
    "prepare": "husky || true"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@rowanmanning/biome-config": "^3.0.1",
    "@types/express": "^5.0.0",
    "express4": "npm:express@^4.21.2",
    "express5": "npm:express@^5.0.1",
    "husky": "^9.0.7",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
  }
}
